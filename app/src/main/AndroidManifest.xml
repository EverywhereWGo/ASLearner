<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.selftest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SelfTest">
        <activity
            android:name=".circleindicator.CircleIndicatorActivity"
            android:exported="false" />
        <activity
            android:name=".magicindicator.MagicIndicatorActivity"
            android:exported="false" />
        <activity
            android:name=".externalstorage.ExternalStorageActivity"
            android:exported="false" />
        <activity
            android:name=".internalstorage.InternalStorageActivity"
            android:exported="false" />
        <activity
            android:name=".xrecyclerview.XRecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name=".swiperecyclerview.SwipeRecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name=".viewpager.ViewPagerActivity"
            android:exported="false" />
        <activity
            android:name=".service.ServiceActivity"
            android:exported="false" />

        <service
            android:name=".service.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".main.RootAcvitity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".notification.NotificationActivity"
            android:exported="false" />
        <activity
            android:name=".notification.MainActivity"
            android:exported="true" />
    </application>

</manifest>